# Frontend Environment Variables
# Copy this file to .env and fill in your values

# Backend API URL
VITE_API_URL=http://localhost:5000

# Frontend Development Bypass Mode
# Set this to 'true' to bypass authentication for frontend development in Codespaces
# This allows developers to access all pages without backend/Azure/MongoDB authentication
# Automatically creates mock user session data
# WARNING: ONLY use this in development/codespace environments, NEVER in production
VITE_DEV_BYPASS=false
